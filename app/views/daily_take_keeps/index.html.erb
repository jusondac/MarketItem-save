<p id="notice"><%= notice %></p>

<h1>Daily Take Keeps</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @daily_take_keeps.each do |daily_take_keep| %>
      <tr>
        <td><%= daily_take_keep.customer_id %></td>
        <td><%= link_to 'Show', daily_take_keep %></td>
        <td><%= link_to 'Edit', edit_daily_take_keep_path(daily_take_keep) %></td>
        <td><%= link_to 'Destroy', daily_take_keep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Daily Take Keep', new_daily_take_keep_path %>
